{
  "scripts": {
    "check:ts": "tsc --version && tsc -p . --pretty --noEmit",
    "clean": "rimraf --glob dist build parcel-bundle-reports .cache .parcel-cache ./src/**/.cache ./src/**/.parcel-cache coverage",
    "dev": "parcel ./src/html/index.html",
    "build": "parcel build dist",
    "start": "parcel serve ./src/html/index.html --open --no-autoinstall",
    "start:server": "parcel serve ./src/html/index.html --no-autoinstall"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "npm-run-all": "4.1.5",
    "parcel": "2.13.3",
    "parcel-bundler": "1.12.5",
    "@parcel/transformer-css": "2.13.3",
    "@parcel/transformer-glsl": "2.13.3",
    "@parcel/transformer-sass": "2.13.3"
  },
  "dependencies": {
    "@babylonjs/core": "7.34.4",
    "@babylonjs/gui": "7.34.4",
    "@babylonjs/loaders": "7.34.4",
    "@babylonjs/materials": "7.34.4",
    "@deck.gl/core": "9.0.35",
    "@loaders.gl/3d-tiles": "4.3.2",
    "@loaders.gl/core": "4.3.2",
    "@loaders.gl/i3s": "4.3.2",
    "@loaders.gl/tiles": "4.3.2",
    "@math.gl/core": "4.1.0",
    "@math.gl/culling": "4.1.0",
    "@math.gl/geospatial": "4.1.0",
    "lodash-es": "^4.17.21",
    "i18next": "^23.9.0"
